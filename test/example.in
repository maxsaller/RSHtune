$molecule
read      anth.mol
$end

$XC_FUNCTIONAL
X HF     0.200
X PBE    0.722                     #1-1/eps
X wPBE   0.078                     #1/eps-alpha
C PBE    1.000
$end

$rem
jobtype=sp
exchange = general
correlation = none
basis= cc-pVTZ
scf_convergence       8
geom_opt_maxcyc       500
max_scf_cycles        100000
mem_static            4000
mem_total             48000
omega                 184       ! Range separation parameter
omega2                184
lrc_dft               true
src_dft               2
HF_SR                 200
HF_LR                 278         
cis_n_roots = 5                  #excited states number
cis_singlets = true               #singlet
cis_triplets = false
cis_state_deriv = 1
pop_mulliken =-1
rpa = 2                           #RPA approximation
GEN_SCFMAN            0
solvent_method        peqs
peqs_switch           3
sym_ignore            true
no_reorient           true
GEN_SCFMAN            0             #old scfman
CIS_DYNAMIC_MEM       TRUE
CIS_RELAXED_DENSITY   TRUE
USE_NEW_FUNCTIONAL    TRUE
ANISO_SRSH_INTERFACE_ATOM           25
$end

$peqs
SolventDiel       3.6
SoluteCavity      RigidVDW
CenterType        COM
GaussBlur         false
BatchSize         9725425 
print             2
$end

$peqs_grid
DimX 161  -6.00    6.00
DimY 161  -6.00    6.00
DimZ 161  -6.00    6.00
$end

